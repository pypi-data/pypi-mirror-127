{
  "name": "emxconvert",
  "version": "0.0.2",
  "main": "index.js",
  "repository": "https://github.com/davidruvolo51/yaml-emx-convert",
  "author": "davidruvolo51",
  "license": "MIT",
  "scripts": {
    "rename": "python3 dev/dev.py",
    "bumpversion:patch": "bump2version --current-version 0.0.2 patch package.json yamlemxconvert/__version__.py setup.py --allow-dirty",
    "bumpversion:minor": "bump2version --current-version 0.0.2 minor package.json yamlemxconvert/__version__.py setup.py",
    "bumpversion:major": "bump2version --current-version 0.0.2 major package.json yamlemxconvert/__version__.py setup.py",
    "build": "python3 setup.py sdist bdist_wheel",
    "check": "twine check dist/*",
    "deploy:prod": "twine upload dist/*",
    "deploy:test": "twine upload --repository testpypi dist/*"
  }
}
